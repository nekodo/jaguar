import * from '//compiler/prelude.jg'

assertEq a b = assert ('expected ' ++ jsonStringify a ++ ' but got ' ++ jsonStringify b) (a == b)

runTests tests = let
  r = map (\t -> t True) tests
  in debug ('Passed ' ++ intToString (length r) ++ ' tests')

testAdd _ = assertEq 8 (4 + 4)

main argv = runTests [
    testAdd
  ]
